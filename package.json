    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "@rsbuild/core": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "consola": "^3.4.2",
    "console-table-printer": "^2.12.1",
    "dotenv-flow": "^4.1.0",
    "got-cjs": "^12.5.4",
    "hostile": "^1.4.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "ora": "5",
    "picocolors": "^1.1.1",
    "qrcode-terminal": "^0.12.0",
    "unplugin": "^2.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "@nuxt/kit": "^3.16.1",
    "@nuxt/schema": "^3.16.1",
    "@rsbuild/core": "^0.4.15",
    "@rspack/core": "^0.5.9",
    "@types/hostile": "^1.3.5",
    "@types/kill-port": "^2.0.3",
    "@types/node": "^20.11.19",
    "@types/proper-lockfile": "^4.1.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@vue/cli": "^5.0.8",
    "bumpp": "^9.11.1",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "fast-glob": "^3.3.3",
    "nodemon": "^3.1.9",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^5.0.10",
    "rollup": "^3.29.4",
    "tsup": "^7.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.0",
    "vite": "^4.5.2",
    "vitest": "^3.0.9",
    "webpack": "^5.90.3"
  }
}
